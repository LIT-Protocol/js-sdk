    "pino": "9.6.0",
    "pino-caller": "4.0.0",
    "siwe": "2.3.2",
    "siwe-recap": "0.0.2-alpha.0",
    "stytch": "12.4.0",
    "syncpack": "13.0.4",
    "tslib": "2.8.1",
    "uint8arrays": "4.0.3",
    "viem": "2.38.3",
    "wagmi": "2.18.1",
    "zod": "3.24.3",
    "zod-validation-error": "3.4.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.4",
    "@nx/esbuild": "21.2.1",
    "@nx/eslint-plugin": "21.2.1",
    "@nx/jest": "21.2.1",
    "@nx/js": "21.2.1",
    "@nx/node": "21.2.1",
    "@nx/plugin": "21.2.1",
    "@solana/web3.js": "1.95.3",
    "@types/depd": "1.1.36",
    "@types/events": "3.0.3",
    "@types/inquirer": "9.0.8",
    "@types/jest": "30.0.0",
    "@types/node": "20.0.0",
    "@types/node-localstorage": "1.3.3",
    "@types/secp256k1": "4.0.6",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "artillery": "2.0.23",
    "axios": "1.6.0",
    "cron-parser": "4.9.0",
    "esbuild": "0.19.2",
    "esbuild-node-builtins": "0.1.0",
    "esbuild-node-externals": "1.14.0",
    "esbuild-plugin-tsc": "0.4.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "ipfs-unixfs-importer": "12.0.1",
    "jest": "30.2.0",
    "jest-environment-jsdom": "29.7.0",
    "node-fetch": "2.6.1",
    "node-localstorage": "3.0.5",
    "nx": "21.2.1",
    "path": "0.12.7",
    "pino-pretty": "13.0.0",
    "prettier": "2.8.8",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsx": "4.20.5",
    "tweetnacl": "1.0.3",
    "typedoc": "0.28.12",
    "typescript": "5.8.3"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ]
}
