{
  "name": "artillery",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/artillery/src",
  "targets": {
    "init": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx packages/artillery/src/init.ts"
      }
    },
    "balance-status": {
      "executor": "nx:run-commands",
      "options": {
        "command": "LOG_LEVEL=${LOG_LEVEL:-silent} tsx packages/artillery/src/balance-status.ts"
      }
    },
    "pkp-sign": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DEBUG_HTTP=true LOG_LEVEL=silent artillery run packages/artillery/configs/pkp-sign.yml"
      }
    },
    "encrypt-decrypt": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DEBUG_HTTP=true LOG_LEVEL=silent artillery run packages/artillery/configs/encrypt-decrypt.yml"
      }
    },
    "execute": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DEBUG_HTTP=true LOG_LEVEL=silent artillery run packages/artillery/configs/execute.yml"
      }
    },
    "mix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DEBUG_HTTP=true LOG_LEVEL=silent artillery run packages/artillery/configs/mix.yml"
      }
    },
    "sign-session-key": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DEBUG_HTTP=true LOG_LEVEL=silent artillery run packages/artillery/configs/sign-session-key.yml"
      }
    }
  },
  "tags": []
}
