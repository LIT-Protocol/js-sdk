{
  "name": "explorer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/explorer/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "cache": false,
      "options": {
        "command": "pnpm --dir apps/explorer dev",
        "forwardAllArgs": true
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": [],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "pnpm --dir apps/explorer build"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --dir apps/explorer lint"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "build"
      ],
      "options": {
        "command": "pnpm --dir apps/explorer start",
        "forwardAllArgs": true
      }
    },
    "docker-build": {
      "dependsOn": [
        "build"
      ],
      "command": "docker build -f apps/explorer/Dockerfile . -t explorer"
    }
  },
  "tags": []
}
